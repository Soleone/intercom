<?xml version="1.0" encoding="UTF-8"?>

<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>com.boourns.intercom</string>

    <key>ProgramArguments</key>
    <array>
      <string>/usr/local/bin/intercom.sh</string>
    </array>

    <key>RunAtLoad</key>
    <true />

    <key>KeepAlive</key>
    <true />

    <key>ExitTimeOut</key>
    <integer>1</integer>

    <key>WorkingDirectory</key>
    <string>/tmp</string>

    <key>StandardErrorPath</key>
    <string>/tmp/intercom.log</string>

    <key>StandardOutPath</key>
    <string>/tmp/intercom.log</string>
  </dict>
</plist>
